    "pinia-plugin-persistedstate": "^3.2.1",
    "regenerator-runtime": "^0.14.1",
    "vant": "^4.8.2",
    "vue": "^3.4.8",
    "vue-router": "^4.2.5",
    "vuex": "^4.1.0",
    "vuex-persist": "^3.1.3",
    "weixin-js-sdk": "^1.6.5"
  },
