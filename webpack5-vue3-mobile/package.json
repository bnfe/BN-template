{
  "name": "webpack",
  "version": "1.0.0",
  "description": "webpack5-vue3-mobile",
  "private": true,
  "scripts": {
    "build:prod": "cross-env NODE_ENV=production BN_ENV=prod webpack --progress --config build/webpack.prod.js",
    "build:test": "cross-env NODE_ENV=production BN_ENV=test webpack --progress --config build/webpack.prod.js",
    "dev": "cross-env NODE_ENV=development BN_ENV=dev webpack serve --progress --config build/webpack.dev.js",
    "lint": "eslint --ext .js,ts,.vue src",
    "format": "prettier --write \"**/*.{js,ts,vue}\"",
    "preinstall": "npx only-allow pnpm"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -e $GIT_PARAMS"
  },
  "lint-staged": {
    "src/**/*.{css,less,scss}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx,vue,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "author": "mirror",
  "dependencies": {
    "@vueuse/core": "^10.7.2",
    "amfe-flexible": "^2.2.1",
    "axios": "^1.6.5",
