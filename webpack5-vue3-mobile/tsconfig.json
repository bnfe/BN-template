{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "~/*": ["./*"],
    },
    "noEmit": true,
    "target": "ES5",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "jsxImportSource": "vue"
  },
  "include": ["src", "types"],
  "exclude": ["node_modules", "dist"]
}
