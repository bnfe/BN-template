{
  "name": "webpack",
  "version": "1.0.0",
  "private": true,
  "description": "webpack5-vue2-admin",
  "scripts": {
    "build:prod": "cross-env NODE_ENV=production BN_ENV=prod webpack --progress --config build/webpack.prod.js",
    "build:test": "cross-env NODE_ENV=production BN_ENV=test webpack --progress --config build/webpack.prod.js",
    "dev": "cross-env NODE_ENV=development BN_ENV=dev webpack serve --progress --config build/webpack.dev.js",
    "lint": "eslint --ext .js,.vue src",
    "format": "prettier --write \"**/*.{js,vue}\""
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -e $GIT_PARAMS"
  },
  "lint-staged": {
    "src/**/*.{css,less,scss}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,vue,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "axios": "^0.26.1",
    "core-js": "^3.17.3",
    "crypto-js": "^3.1.8",
    "element-ui": "2.15.6",
    "nprogress": "^0.2.0",
    "regenerator-runtime": "^0.13.9",
    "vue": "2.6.14",
    "vue-router": "^3.5.2",
    "vuex": "^3.6.2",
    "vuex-persist": "^3.1.3",
    "xlsx": "^0.17.4"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/eslint-plugin": "^7.17.7",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/plugin-transform-object-rest-spread": "^7.23.4",
    "@babel/plugin-transform-runtime": "^7.23.7",
    "@babel/preset-env": "^7.15.6",
    "@babel/runtime": "^7.15.4",
    "@vue/babel-preset-jsx": "^1.2.4",
    "babel-loader": "^8.2.2",
    "babel-plugin-component": "^1.1.1",
    "clean-webpack-plugin": "^4.0.0",
    "commitlint": "^17.6.3",
    "commitlint-config-gitmoji": "^2.3.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "dayjs": "^1.10.7",
    "dotenv": "^16.0.0",
    "dotenv-expand": "^8.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^7.17.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "interpolate-html-plugin": "^4.0.0",
    "lint-staged": "^13.2.2",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.2.2",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "path-to-regexp": "^6.2.0",
    "portfinder": "^1.0.28",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^7.4.3",
    "prettier": "^2.8.8",
    "qs": "^6.10.5",
    "sass": "1.32.13",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "terser-webpack-plugin": "^5.2.4",
    "thread-loader": "^3.0.4",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "2.6.14",
    "webpack": "^5.52.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.0",
    "webpack-merge": "^5.8.0",
    "yorkie": "^2.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Android >= 4.0",
    "iOS >= 6"
  ]
}
